<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.timer.presentation.fragmentWebView.activity.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.example.user.Timer.presentation.View.LoadingView
            android:id="@+id/loadingView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />

        <com.example.user.Timer.presentation.View.CustomScrollViewForGraphics
            android:id="@+id/customScrollViewForGraphics"
            android:layout_width="match_parent"
            android:layout_height="300dp">

        </com.example.user.Timer.presentation.View.CustomScrollViewForGraphics>
    </LinearLayout>
</layout>