<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="4dp"
        android:top="1.5dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#ff33b5e5" />
            </shape>
        </rotate>
    </item>

    <item
        android:width="40dp"
        android:height="4dp"
        android:left="19dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#ff33b5e5" />
            </shape>
        </rotate>
    </item>

</layer-list>